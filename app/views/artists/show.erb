<h1>Artist's Show Page</h1>

<p><%= @artist.first_name %></p>
<p><%= @artist.last_name %></p>
<p><%= @artist.alias %></p>

<a href="/artists/<%= @artist.id %>/edit">Edit Artist</a>

<form action="<%= @artist.id %>"  method="post">
  <input name="_method" type="hidden" value="delete">
  <input type="submit" value="Delete Artist">
</form>